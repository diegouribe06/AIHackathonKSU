{
  "name": "fly-import",
  "version": "1.0.0",
  "keywords": [
    "npm",
    "npx",
    "import",
    "loaders",
    "on-demand"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "xo",
    "prepare": "npm run build",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@npmcli/arborist": "^9.1.3",
    "env-paths": "^3.0.0",
    "registry-auth-token": "^5.1.0",
    "registry-url": "^7.2.0"
  },
  "devDependencies": {
    "@node-loaders/esbuildx": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/npmcli__arborist": "^6.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "xo": "^1.2.2"
  },
  "engines": {
    "node": "^20.17.0 || >=22.9.0"
  }
}
